{
    "name": "kaadon/kaadon_admin_server",
    "description": "bool_admin framework for thinkphp8.*",
    "type": "project",
    "keywords": [
        "bool_admin",
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "http://developer.kaadon.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        },
        {
            "name": "kaadon.com",
            "email": "kaadon.com@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "topthink/framework": "^v8.0",
        "topthink/think-orm": "^3.0",
        "topthink/think-multi-app": "^1.0",
        "topthink/think-view": "^2.0",
        "topthink/think-filesystem": "^2.0",
        "topthink/think-queue": "^3.0",
        "symfony/var-exporter": "^6.3",
        "kaadon/uploadfile": "^v0.0.1",
        "kaadon/thinkbase": "^1.0",
        "kaadon/capcha": "^2.0",
        "kaadon/think-addons": "^3.0",
        "kaadon/uuid": "^1.0",
        "react/event-loop": "^1.5",
        "usdtcloud/tronservice": "^2.0",
        "kaadon/office": "dev-master",
        "ext-redis": "*",
        "ext-curl": "*",
        "ext-yaconf": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "topthink/think-trace": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app",
            "resources\\": "resources"
        },
        "psr-0": {
            "": "extend/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    }
}
